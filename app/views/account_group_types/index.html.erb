<h1>Listing account_group_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>...</th>
  </tr>

<% @account_group_types.each do |account_group_type| %>
  <tr>
    <td><%= account_group_type.name %></td>
    <td><%= link_to 'Show', account_group_type %> | 
        <%= link_to 'Edit', edit_account_group_type_path(account_group_type) %> | 
