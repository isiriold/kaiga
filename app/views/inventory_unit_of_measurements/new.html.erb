  <p>
    <%= f.label :unit_name %><br />
    <%= f.text_field :unit_name %>
  </p>
  <p>
    <%= f.label :unit_symbol %><br />
    <%= f.text_field :unit_symbol %>
  </p>
  <p>
    <%= f.label :sub_unit_name %><br />
